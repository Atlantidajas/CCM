        </LinearLayout>

        <!-- Hasta Expense -> Vehicle -> brand 13-->
        <LinearLayout
            android:id="@+id/layout_vehicleModelVehicle_expense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView_vehicleModelVehicle_expense"
                android:layout_width="42dp"
                android:layout_height="42dp"
                android:src="@mipmap/ic_launcher_car"
                android:layout_marginHorizontal="40dp"
                android:layout_marginVertical="10dp" />


            <TextView
                android:id="@+id/textView_vehicleModelVehicle_expense"
